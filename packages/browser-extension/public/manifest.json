{
  "name": "Work Item Search for Azure DevOps",
  "description": "Instanly retrieve work items by title or metadata",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "options_page": "options.html",
  "permissions": ["storage"],
  "host_permissions": ["https://dev.azure.com/"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+A"
      }
    }
  }
}
